{
    "jshint.enable": false,
    "eslint.autoFixOnSave": true,
    "eslint.enable": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact"
    ],
    "todohighlight.isEnable": true,
      "todohighlight.isCaseSensitive": true,
      "todohighlight.keywords": [
          "DEBUG:",
          "REVIEW:",
          {
              "text": "NOTE:",
              "color": "#ff0000",
              "backgroundColor": "yellow",
              "overviewRulerColor": "grey"
          },
          {
              "text": "HACK:",
              "color": "#000",
              "isWholeLine": false
          }
      ]
  }